{
  "manifest_version": 2,

  "name": "DKMH",
  "description": "Tool dkmh",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "page_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Tool Dkmh"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "content_scripts":[
    {
      "matches": ["http://thongtindaotao.sgu.edu.vn/Default.aspx?page=dkmonhoc/*"],
      "js": ["lib.js","content.js" ],
      "css": ["content.css"]
    }
  ],

  "permissions": [
    "tab" ,
    " http://thongtindaotao.sgu.edu.vn/Default.aspx?page=dkmonhoc/* "
        ]
}
